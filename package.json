{
  "name": "nextjs_lab1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "env-cmd -f .env.dev prisma generate",
    "db:migrate": "env-cmd -f .env.dev prisma migrate dev",
    "db:migrate:prod": "env-cmd -f .env prisma migrate dev",
    "db:studio": "env-cmd -f .env.dev prisma studio",
    "db:studio:prod": "env-cmd -f .env prisma studio",
    "db:seed": "env-cmd -f .env.dev ts-node prisma/seed.ts",
    "db:seed:prod": "env-cmd -f .env ts-node prisma/seed.ts",
    "db:seed:hard": "pnpm run db:reset && pnpm run db:seed",
    "db:reset": "env-cmd -f .env.dev ts-node prisma/db-reset.ts",
    "db:reset:prod": "env-cmd -f .env ts-node prisma/db-reset.ts",
    "db:push": "env-cmd -f .env.dev prisma db push",
    "db:push:prod": "env-cmd -f .env.prod prisma db push"
  },
  "dependencies": {
    "@next/env": "^14.2.3",
    "@prisma/client": "^5.13.0",
    "@vercel/postgres": "^0.8.0",
    "env-cmd": "^10.1.0",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "ts-node": "^10.9.2",
    "zod-prisma": "^0.5.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
